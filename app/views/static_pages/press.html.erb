<% @page_title = "Press" %>
<% @page_description = "" %>

<div class="event-lay press">
	<%= render 'shared/collage-hd' %>
	<div class="inside">
		<div class="title">
			<h1><%= @page_title %></h1>
		</div>
	</div>

	<div class="event-list">
		<div class="inside med-def">
			<%= link_to 'https://digital.modernluxury.com/publication/?i=522504&ver=html5&p=60#{%22page%22:60,%22issue_id%22:522504}', 
									class: 'event-li article', target: '_blank' do %>
				<%= image_tag 'ml-jez.png', class: 'img-responsive article-img', alt: '' %>
				<div class="textbox">
					<p class="pub">Modern Luxury</p>
					<h4>Wonder Woman</h4>
					<p class="sub">fab'rik's fab leading lady, Dana Spinola, shows us how she wears a hero piece three ways for the ultimate fall-style inspo.</p>
					<p class="text-link">View Article</p>
				</div>
			<% end %>

			<%= link_to 'https://www.forbes.com/sites/robertreiss/2018/01/16/five-great-american-entrepreneurs-you-havent-yet-heard-of-but-should-know/#ce825a755d71', 
									class: 'event-li article', target: '_blank' do %>
				<%= image_tag 'dana-bio5.jpg', class: 'img-responsive', alt: '' %>
				<div class="textbox">
					<p class="pub">Forbes</p>
					<h4>Five Great American Entrepreneurs You Haven't Yet Heard Of But Should Know</h4>
					<p class="text-link">View Article</p>
				</div>
			<% end %>
		</div>
	</div>
</div>
